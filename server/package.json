{
  "name": "wise-old-man-server",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "lint": "npx eslint@7.2 . --ext .ts",
    "prestart": "sequelize db:migrate",
    "start": "pm2-runtime dist/server.js -i 4",
    "predev": "npm run build && sequelize db:migrate",
    "dev": "ts-node-dev --poll --respawn --transpile-only --ignore-watch node_modules src/server.ts",
    "pretest": "cross-env NODE_ENV=test sequelize db:migrate",
    "test": "cross-env NODE_ENV=test jest __tests__/integration --detectOpenHandles --verbose",
    "posttest": "cross-env NODE_ENV=test sequelize db:migrate:undo:all",
    "migration:generate": "cross-env MIGRATING=false npx sequelize-cli migration:generate --name "
  },
  "author": "Psikoi",
  "license": "ISC",
  "dependencies": {
    "@sentry/node": "^5.24.2",
    "@sentry/tracing": "^5.24.2",
    "axios": "^0.19.2",
    "bcrypt": "^5.0.0",
    "bull": "^3.16.0",
    "cheerio": "^1.0.0-rc.3",
    "cheerio-tableparser": "^1.0.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "csvtojson": "^2.0.10",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.3",
    "lodash": "^4.17.20",
    "moment": "^2.26.0",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.13",
    "sequelize-typescript": "^1.1.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.32",
    "@types/bull": "^3.14.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/express-rate-limit": "^5.0.0",
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.159",
    "@types/node": "^14.0.11",
    "@types/supertest": "^2.0.10",
    "@types/validator": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.7.1",
    "faker": "^4.1.0",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "sequelize-cli": "^5.5.1",
    "sqlite3": "^5.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.4",
    "ts-node-dev": "^1.0.0-pre.62",
    "typescript": "^3.9.7"
  }
}
