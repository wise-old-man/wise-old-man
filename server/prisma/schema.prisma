  player Player @relation(fields: [playerId], references: [id], onDelete: Cascade, onUpdate: NoAction)

  // Constraints
  @@id([playerId, period])
  // Indices
  @@index([period, overall(sort: Desc)], map: "deltas_period_overall")
  // Map
